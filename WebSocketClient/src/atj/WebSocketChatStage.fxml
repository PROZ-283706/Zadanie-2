<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minHeight="400" minWidth="600" prefHeight="450.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="atj.WebSocketChatStageController">
   <children>
   	  <Label layoutX="54.0" layoutY="31.0" text="User:" />
      <Label layoutX="54.0" layoutY="191.0" text="Chat:" />
      <Label layoutX="43.0" layoutY="349.0" text="Message:" />
      <Label layoutX="517.0" layoutY="81.0" text="User name is:" />
      <Button fx:id="btnSet" layoutX="516.0" layoutY="32.0" mnemonicParsing="false" onAction="#btnSet_Click" prefHeight="27.0" prefWidth="69.0" text="Set"  />
      <Button fx:id="btnSend" disable="true" layoutX="482.0" layoutY="347.0" mnemonicParsing="false" onAction="#btnSend_Click" prefHeight="26.0" prefWidth="70.0" text="Send" />
      <TextField fx:id="messageTextField" layoutX="140.0" layoutY="348.0" onKeyPressed="#TextField_KeyPressed" prefHeight="26.0" prefWidth="327.0" />
      <TextField fx:id="userTextField" layoutX="141.0" layoutY="32.0" prefHeight="28.0" prefWidth="328.0" />
      <TextArea fx:id="userNameArea" editable="false" layoutX="490.0" layoutY="101.0" prefHeight="42.0" prefWidth="125.0" />
      <Label layoutX="523.0" layoutY="169.0" text="File to send:" />
