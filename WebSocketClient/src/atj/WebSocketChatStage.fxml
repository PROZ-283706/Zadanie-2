      <TextArea fx:id="chatTextArea" editable="false" layoutX="142.0" layoutY="81.0" prefHeight="257.0" prefWidth="326.0" />
      <Button fx:id="btnSendFile" disable="true" layoutX="565.0" layoutY="285.0" mnemonicParsing="false" onAction="#btnUploadFile_Click"  prefHeight="26.0" prefWidth="70.0" text="Upload" />
      <Button fx:id="btnDeleteFile" disable="true" layoutX="482.0" layoutY="285.0" mnemonicParsing="false" onAction="#btnDeleteFile_Click"  prefHeight="26.0" prefWidth="70.0" text="Delete" />
      <Button fx:id="btnSelectFile" disable="true" layoutX="565.0" layoutY="347.0" mnemonicParsing="false" onAction="#btnSelectFile_Click" onKeyPressed="#btnSelectFile_Click" prefHeight="26.0" prefWidth="70.0" text="Select File" />
   </children>
</AnchorPane>